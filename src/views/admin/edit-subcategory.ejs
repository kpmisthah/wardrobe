 <%-include("../../views/partials/adminPartials/headers")%>
 <div class="container">
     <div class="row justify-content-center">
         <div class="col-md-6 mt-60">
  
             <form method="POST" action="/admin/edit-sub/<%=subcategory._id%>">
                 <div class="mb-4 mt-30">
                    <label for="product_name" class="form-label">Name</label>
                    <input type="text" name="categoryName" value="<%=subcategory.name%>" class="form-control border" />
                 </div>
 
                 <div class="mb-4">
                    <label class="form-label">Description</label>
                    <textarea name="description" class="form-control border"><%=subcategory.description%></textarea>
                 </div>
 
                 <div class="d-grid text-end mt-30 col-md-5">
                    <button class="btn btn-primary" type="submit">Update subcategory</button>
                 </div>
             </form>
         </div>
     </div>
 </div>
 
 
